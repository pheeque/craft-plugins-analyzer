{
    "name": "pheeque/craft-plugins-analyzer",
    "description": "Tool to analyze data about available Craft plugins",
    "homepage": "https://github.com/pheeque/craft-plugins-analyzer",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pheeque\\CraftPluginsAnalyzer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rafik",
            "email": "contact@pheeque.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/collections": "*",
        "symfony/console": "^5.4"
    },
    "bin": [
        "bin/craft-plugins-analyzer"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^1.21"
    }
}
